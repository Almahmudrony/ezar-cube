<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ezartech.demos.cube" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ezAR-Cube</name>
    <description>
        ezAR rotating cube on video preview using three.js
    </description>
    <author email="info@ezartech.com" href="http://ezartech.com">
        ezAR
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
